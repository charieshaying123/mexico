    <script>
        function toggleMenu() {
            const navLinks = document.querySelector('.nav-links');
            navLinks.classList.toggle('active');
        }
     // Function to animate the counter
const counters = document.querySelectorAll('.counter');

counters.forEach(counter => {
    const target = +counter.getAttribute('data-target'); // Get the target number
    let count = 0; // Start from 0

    // Function to update the counter value
    const updateCounter = () => {
        const increment = target / 100; // Increment value to reach the target smoothly
        if (count < target) {
            count += increment;
            counter.textContent = Math.ceil(count); // Set the new count value
            requestAnimationFrame(updateCounter); // Keep updating until the target is reached
        } else {
            counter.textContent = target; // Ensure we end with the exact target value
        }
    };

    setTimeout(() => {
        updateCounter(); // Start the counter animation after the delay
    }, 500); // Adjust the delay time as needed (in milliseconds)
});


    </script>
